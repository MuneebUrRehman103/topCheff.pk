<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.themuneeb.myfristapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StandardMenuActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".PhoneNoLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CateringActivity" />
        <activity android:name=".SideBarActivity" />
        <activity android:name=".MenuDetailsActivity"></activity>
        <activity android:name=".CartMenuActivity" />
        <activity
            android:name=".GeneralMenuForSelectedCategoryActivity"
            android:label="@string/title_activity_general_menu_for_selected_category_activity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".MainMenuActivity"></activity>
    </application>

</manifest>